        "autoprefixer": "^10.4.19",
        "bits-ui": "^0.21.12",
        "clsx": "^2.1.1",
        "daisyui": "^4.11.1",
        "dotenv": "^16.4.5",
        "dotenv-cli": "^7.4.2",
        "hardhat": "^2.22.10",
        "just-extend": "^6.2.0",
        "markdown-it": "^14.1.0",
        "mdsvex": "^0.11.2",
        "mocha": "^10.7.3",
        "path-browserify": "^1.0.1",
        "postcss": "^8.4.38",
        "postcss-import": "^16.1.0",
