        "@sveltejs/adapter-static": "^3.0.2",
        "@sveltejs/kit": "^2.0.0",
        "@sveltejs/vite-plugin-svelte": "^3.0.0",
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.13",
        "@types/file-saver": "^2.0.7",
        "@types/markdown-it": "^14.1.2",
        "@types/mocha": "^10.0.8",
        "@types/node": "^22.5.4",
        "@types/path-browserify": "^1.0.3",
        "@types/uuid": "^9.0.8",
